.list-wrap {
  margin-bottom: 16rpx;
  padding: 0 24rpx;
  background: #fff;
  border-top: 0.5px solid #eee;
}
.list {
  height: 100rpx;
  border-bottom: 0.5px solid #eee;
}
.list:last-child {
  border-bottom: none;
}
.title {
  color: #333;
  font-size: 28rpx;
}
.content {
  color: #666;
  font-size: 26rpx;
}
.arrow {
  margin-left: 6rpx;
  width: 24rpx;
  height: 24rpx;
}
